{
	"manifest_version": 2,

	"name": "OGINT",
	"description": "This extension opens Google.com in new tab",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Open Google in new tab"
	},
	"background":{
  		"scripts": ["background.js"]
	},
	"permissions": [
		"tabs"
	]	
}